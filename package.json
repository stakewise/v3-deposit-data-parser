{
  "type": "module",
  "version": "1.10.1",
  "main": "dist/index.js",
  "name": "@stakewise/v3-deposit-data-parser",
  "description": "v3-deposit-data-parser",
  "homepage": "https://github.com/stakewise/v3-deposit-data-parser",
  "sideEffects": false,
  "types": "dist/index.d.ts",
  "license": "AGPL-3.0-only",
  "typings": "dist/index.d.ts",
  "module": "dist/index.esm.js",
  "scripts": {
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "test:ci": "NODE_OPTIONS=--experimental-vm-modules jest --runInBand",
    "lint": "eslint src --ext .ts,.tsx,.js,.jsx",
    "build": "pnpm test:ci && pnpm prepare && rm -rf ./dist && pnpm rollup",
    "rollup": "rollup --config rollup.config.js",
    "beforePublish": "node ./scripts/beforePublish",
    "prepare": "husky && pnpm beforePublish",
    "publish": "pnpm build && pnpm publish --tag=latest"
  },
  "files": [
    "dist/",
    "package.json"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.esm.js"
    },
    "./index.js": "./dist/index.js",
    "./index.js.map": "./dist/index.js.map",
    "./index.esm.js": "./dist/index.esm.js",
    "./index.esm.js.map": "./dist/index.esm.js.map",
    "./package.json": "./package.json"
  },
  "devDependencies": {
    "@chainsafe/ssz": "1.2.0",
    "@eslint/js": "9.28.0",
    "@jest/globals": "30.2.0",
    "@openzeppelin/merkle-tree": "1.0.8",
    "@rollup/plugin-commonjs": "28.0.3",
    "@rollup/plugin-json": "6.1.0",
    "@rollup/plugin-node-resolve": "16.0.1",
    "@rollup/plugin-terser": "0.4.4",
    "@rollup/plugin-typescript": "12.1.2",
    "@types/jest": "30.0.0",
    "@types/rollup-plugin-peer-deps-external": "2.2.5",
    "@typescript-eslint/eslint-plugin": "8.33.0",
    "@typescript-eslint/parser": "8.33.0",
    "bls-eth-wasm": "1.4.0",
    "eslint": "9.27.0",
    "eslint-import-resolver-typescript": "4.4.2",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jest": "28.12.0",
    "globals": "16.2.0",
    "husky": "9.1.7",
    "jest": "30.2.0",
    "rollup": "4.41.1",
    "rollup-plugin-delete": "3.0.1",
    "rollup-plugin-dts": "6.2.1",
    "rollup-plugin-peer-deps-external": "2.2.4",
    "ts-jest": "29.4.6",
    "ts-node": "10.9.2",
    "tslib": "2.8.1",
    "typescript": "5.8.3"
  }
}
